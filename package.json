{
  "name": "order-trace",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prepare": "husky install",
    "mock-api": "json-server --watch mocks/orders.json --port 3001 --middlewares mocks/mock-api.cjs",
    "cypress:open": "cypress open",
    "cy:open-e2e": "cypress open --e2e --browser chrome",
    "cy:open-unit": "cypress open --component --browser chrome",
    "cy:run-e2e": "cypress run --e2e",
    "cy:run-unit": "cypress run --component",
    "cy:e2e": "start-server-and-test start http-get://localhost:3000 cy:open-e2e"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.1",
    "@react-google-maps/api": "^2.19.2",
    "axios": "^1.5.1",
    "jotai": "^2.4.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.47.0",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.16.0",
    "react-toastify": "^9.1.3",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@testing-library/cypress": "^9.0.0",
    "@types/cypress": "^1.1.3",
    "@types/mocha": "^10.0.2",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.16",
    "cypress": "^12.9.0",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "postcss": "^8.4.31",
    "prettier": "3.0.3",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn precommit"
    }
  }
}
